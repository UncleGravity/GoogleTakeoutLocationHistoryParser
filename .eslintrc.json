{
    "plugins": ["prettier"],
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "prettier/prettier": "error",
        "indent": ["error", 4],
        "import/prefer-default-export": "off",
        "no-param-reassign": "off",
        "no-plusplus": "off",
        "import/extensions": 0
    }
}
